{
  "name": "ticktackfour-api",
  "version": "1.0.0",
  "description": "Azure Functions backend for TickTackFour multiplayer game timer",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"npm start\"",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "func start --verbose"
  },
  "dependencies": {
    "@azure/data-tables": "^13.3.0",
    "@azure/functions": "^4.8.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
